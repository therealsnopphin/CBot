    "default": 0.1,
    "min": 0.0,
    "max": 999,
    "controls": {
      "arrows": true,
      "arrow-step": 0.01,
      "slider": true,
      "slider-step": 0.01,
      "input": false
    }
  },
  "Max softclicks_time": {
    "type": "float",
    "name": "Max Softclicks Time",
    "description": "Maximum delay between soft clicks in seconds.",
    "default": 0.1,
    "min": 0.0,
    "max": 999,
    "controls": {
      "arrows": true,
      "arrow-step": 0.01,
      "slider": true,
      "slider-step": 0.01,
      "input": false
    }
  },

  "Min hardclicks_time": {
    "type": "float",
    "name": "Min Hardclicks Time",
    "description": "Minimum delay between hard clicks in seconds.",
    "default": 2,
    "min": 0.0,
    "max": 999,
    "controls": {
      "arrows": true,
      "arrow-step": 0.01,
      "slider": true,
      "slider-step": 0.01,
      "input": false
    }
  },
  "Max hardclicks_time": {
    "type": "float",
    "name": "Max Hardclicks Time",
    "description": "Maximum delay between hard clicks in seconds.",
    "default": 2,
    "min": 0.0,
    "max": 999,
    "controls": {
      "arrows": true,
      "arrow-step": 0.01,
      "slider": true,
      "slider-step": 0.01,
      "input": false
    }
  },

  "Reverb effect": {
    "type": "int",
    "name": "Reverb Effect",
    "description": "The selected reverb effect for clicks index.",
    "default": 0,
    "min": 0,
    "max": 33
  },

  "Random panning": {
    "type": "bool",
    "name": "Random Panning",
    "description": "If enabled, each click will be heard randomly, like left, middle, right",
    "default": false
  }
},

	"links": { "homepage": "https://twoorsetp.github.io/cbotliveultimate/index.html", "source": "https://github.com/therealsnopphin/CBot", "community": "https://discord.gg/N2bxXBsqEJ" }
}
